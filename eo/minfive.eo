+package sandbox
+alias stdout org.eolang.io.stdout
+alias sprintf org.eolang.txt.sprintf

[args...] > app
  memory > min
  memory > temp
  memory > x
  seq > @
    min.write (args.get 0).toInt
    x.write 0
    while.
      x.less 5
      [i]
        seq > @
          temp.write (args.get i).toInt
          (temp.less min).if
            min.write (temp)
            false
          x.write (x.add 1)
    stdout
      sprintf "Minimum number is %d\n" min
